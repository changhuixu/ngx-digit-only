{
  "name": "ngx-digit-only",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:gh-pages": "ng build --base-href /ngx-digit-only/",
    "build:lib": "ng build --project=@uiowa/digit-only",
    "cypress:open": "start-server-and-test start http://localhost:4200 \"cypress open\"",
    "cypress:verify": "cypress verify",
    "cypress:run": "start-server-and-test start http://localhost:4200 \"cypress run\""
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.4.1",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@types/node": "^24.10.1",
    "cypress": "^14.3.3",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.2"
  }
}
